function destroyer(arr) {
  // Remove all the values

  //copy the entire set of arguments into an array using the spread operator
  var args = Array.from(arguments);
  var args = [...arguments];
//print out to test
  console.log(args);
  
//filter out the difference to make sure all the is returned is the difference
  return arr.filter(x => !args.includes(x) | !args.includes(x));

}

destroyer([1, 2, 3, 1, 2, 3], 2, 3);
